<h1>All Products</h1>
<%= link_to "New Product", new_product_path, class: "btn btn-primary"  %>
<table class="table">
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Part Number</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @products.each do |p| %>
    <tr>
      <td><%= p.id %></td>
      <td><%= p.name %></td>
      <td><%= p.part_number %></td>
      <td><%= link_to "Show", p %></td>
      <td><%= link_to "Edit", edit_product_path(p) %></td>
      <td><%= link_to "Delete", p, method: "delete", data: { confirm: 'Are you sure?' } %></td>
    </tr>
    <% end %>
  </tbody>
</table>